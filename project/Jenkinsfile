pipeline {
     agent any
     stages {
	     stage("Build") {
               steps {
                   sh 'touch file1'
		       sh 'ls'
               }
          }
          stage('deploy stage Stage') {
               steps {
		       script{
			     
                       def server = Artifactory.server 'artifactory server'
	               
                       def uploadSpec = """{
  		"files": [
    			{
      			"pattern": "emma1",
            		"target": "MyArtRepo/${BUILD_NUMBER}/"
    				}
 				]
			}"""
		       server.upload(uploadSpec)
          }
       }
      }
     }
}
