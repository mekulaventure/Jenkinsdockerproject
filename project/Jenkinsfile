pipeline {
     agent any
     stages {
          stage("checkout") {
               steps {
                 git url: 'https://github.com/mekulaventure/Jenkinsdockerproject.git'
               }
          }
	     stage("Build") {
               steps {
                   sh 'touch file1'
               }
          }
          stage('deploy stage Stage') {
               steps {
		       script{
			     
                       def server = Artifactory.server 'artifactory server'
	               
                       def uploadSpec = """{
  		"files": [
    			{
      			"pattern": "emma1",
            		"target": "MyArtRepo/${BUILD_NUMBER}/"
    				}
 				]
			}"""
		       server.upload(uploadSpec)
          }
       }
      }
     }
}
